		"@pixi/filter-pixelate": "^4",
		"@pixi/filter-rgb-split": "^4",
		"@pixi/filter-shockwave": "^4",
		"@pixi/graphics": "6.4.2",
		"@pixi/interaction": "6.4.2",
		"@pixi/loaders": "6.4.2",
